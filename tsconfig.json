{
  "compilerOptions": {
    "target": "es2021",
    "module": "commonjs",
    "outDir": "dist",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "checkJs": true,
	  "moduleResolution": "node",
	  "types": ["node"],
  },
  "include": ["nodes/**/*", "credentials/**/*"]
}